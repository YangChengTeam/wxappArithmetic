/* pages/start/start.wxss */
.start-content {
   background-color: #8373c8;
   width: 100%;
   height: 700rpx;
   position: relative;
   border-radius: 25rpx;
   overflow: hidden;

}

.time-content {
   background-color: #775ee0;
   width: 100%;
   height: 685rpx;
   border-radius: 25rpx;
   position: absolute;
}


.game-content {
   display: flex;
   justify-content: center;
   background-color: #fff;
   width: 100%;
   height: 685rpx;
   border-radius: 25rpx;
   position: absolute;
   top: -685rpx;
   opacity: 0;
}


.time-num {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top:0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 330rpx;
  
}

.time-num > view {
    text-align: center;
    width: 100%;
    opacity: 0;
}

.start-desp > .title {
    font-size: 48rpx;
    width: 100%;
    text-align: center;
}

.start-desp > .desp {
    font-size: 30rpx;
    width: 100%;
    text-align: center;
    margin-top: 10rpx;
}

.game-content-wrap { 
   display: flex;
   flex-direction: column;
   align-items: center;
   width: 580rpx;
   height: 100%;
   color: black;
}

.process-info {
   width: 100%;
   height: 26rpx;
   margin-top: 70rpx;
}

.process-info-wrap {
  position: relative;
  width: 100%;
  height: 26rpx;
}

.process-info-wrap > image {
   position: absolute;
   width: 100%;
   height: 26rpx;
}

.process-info-wrap > .process {
    width: 100%;
    height: 26rpx;
    position: absolute;
    background: url('data:image/png;base64,');
    background-size: cover;
}
.question-in-all {
   font-size: 30rpx;
   margin-top: 40rpx;
}

.question-index {
  display: flex;
  width: 100%;
  height: 40rpx;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}

.question-index > .line {
   width: 80rpx;
   height: 2rpx;
   background-color: #382198;
}

.question-index > .title {
   font-size: 36rpx;
   margin-left: 30rpx;
   margin-right: 30rpx;
}

.question-info {
   display: flex;
   justify-content: center;
   align-items: center;
   width: 100%;
   height: 330rpx;
   background-color: #ffebc1;
}

.question-info {
  font-size: 90rpx;
  margin-top: 30rpx;
}

.start-footer {
   width: 100%;
   height: 315rpx;
   margin-top: 90rpx;
}

.start-footer-wrap {
   position: relative;
   width: 100%;
   height: 315rpx;
   overflow: hidden;
}

.start-desp {
   position: absolute;
   width: 100%;
}

.game-select {
   position: absolute;
   width: 100%;
   height: 315rpx;
   display: flex;
   flex-direction: row;
   justify-content: space-between;
   top: 315rpx;
   opacity: 0;
}

.game-select image {
   height: 315rpx;
   width: 300rpx;
}


.mask {
   position: fixed;
   width: 100%;
   height: 100%;
   background: rgba(0,0,0,0.7);
   display: flex;
   flex-direction: column;
   align-items: center;
   top:0;
   left:0;
}

.mask-content {
   display: flex;
   flex-direction: column;
   width: 510rpx;
   height: 100%;
   align-items: center;
   top: -100%;
   opacity: 0;
   position: relative;
}


.mask-main-content {
   display: flex;
   flex-direction: column;
   justify-content:flex-start;
   align-items: center;
   width: 510rpx;
   height: 450rpx;
   background-color: #fff;
   border-radius: 15rpx;
   margin-top: 140rpx;
}


.fail-img2 {
   width: 485rpx;
   height: 193rpx;
   position: relative;
   z-index: 2;
   top: 190rpx;
   left: 30rpx;
}

.succ  .mask-main-content {
   height: 722rpx;
   width: 512rpx;
}
.succ {
   width: 512rpx;
   margin-top: 140rpx;
}

.mask-content > .close {
   width: 48rpx;
   height: 48rpx;
   margin-top: 50rpx;
   
}

.mask-main-content > .title {
   font-size: 32rpx;
   color: #000;
   width: 80%;
   margin-top: 60rpx;
   text-align: center;
}

.succ .title {
   margin-top: 36rpx;
   color: #fff;
   font-size: 40rpx;
}

.succ .avatar {
   width: 180rpx;
   height: 180rpx;
   margin-top: 20rpx;
   border-radius: 50%;
}

.succ .desp {
   font-size: 30rpx;
   margin-top: 20rpx;
}

.succ .desp2 {
   font-size: 36rpx;
   margin-top: 10rpx;
   color: #ecc47f;
}

.open-money {
   margin-top: 40rpx;
   width: 182rpx;
   height: 182rpx;
   opacity: 0;
}

.mask-main-content > .continue {
   font-size: 30rpx;
   height: 90rpx;
   width: 380rpx;
   border-radius: 45rpx;
   line-height: 90rpx;
   background: #775ee0;
   color: #fff;
   margin-top: 30rpx;
}

.mask-main-content > .invate-friend {
   font-size: 30rpx;
   height: 90rpx;
   width: 380rpx;
   border-radius: 45rpx;
   line-height: 90rpx;
   background-color: #fff;
   border: solid 2rpx #775ee0;
   color: #775ee0;
   margin-top: 30rpx;
}

.succ > .mask-main-content {
  margin-top: 0;
  background: rgba(0,0,0,0.0);
  z-index: 100;
}

.mask-content > .succ-header {
   width: 512rpx;
   height: 722rpx;
   position: absolute;
   top: 0;
}

.mask > .win-header {
   width: 750rpx;
   height: 500rpx;
   position: absolute;
   z-index: -1;
   top:170rpx;
   left: 0rpx;
  
}

.mask-succ {
   opacity: 0;
   top: -100%;
}

.mask-fail {
   opacity: 0;
   top: -100%;
}


.ig {
   animation:circle 1.5s infinite linear;
}
@keyframes circle{
     0%{ transform:rotate(0deg); }
     100%{ transform:rotate(360deg); }
}

.mask-share {
   left: 0;
   top: -100%;
}

.main-share {
   display: flex;
   width: 500rpx;
   height: 500rpx;
   flex-direction: column;
   align-items: center;
   border-radius: 10rpx;
   margin-top: 140rpx;
   position: relative;
}



.share-part {
   display: flex;
   width: 100%;
   height: 300rpx;
   flex-direction: column;
   align-items: center;
   position: relative;
   z-index: 100rpx;
}

.share-part2 {
   width: 100%;
   height: 200rpx;
   background: #fff;
    justify-content: center;
    display: flex;
   border-bottom-left-radius: 15rpx;
   border-bottom-right-radius: 15rpx;
  
}

.share-part2-wrapper {
   position: relative;
   width: 378rpx;
   height: 90rpx;
   margin-top: 50rpx;
}

.share-part2-wrapper  > image {
   width: 378rpx;
   height: 90rpx;
   position: absolute;
   left: 0;
   top: 0;
}

.share-part2-wrapper  > button {
    position: absolute;
     left: 0;
   top: 0;
    background: linear-gradient(to right, #d866db, #e565b1,#e64690, #e84081, #ee4f62); 
    width: 378rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 40rpx;
    color: #fff;
    font-size: 30rpx;
    opacity: 0;
}

.share-img {
   width: 500rpx;
   height: 300rpx;
   position: absolute;
   left: 0;
   top: 0;
}

.share-more {
   width: 390rpx;
   height: 66rpx;
   margin-top: 60rpx;
   margin-bottom: 20rpx;
}

.share-title {
   color: #f8d548;
   font-size: 36rpx;
}

.share-desp {
   color: #fff;
   font-size: 26rpx;
   margin-top: 15rpx;
}